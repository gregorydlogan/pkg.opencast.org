[opencast-testing]
name = Opencast {{ os }} {{ version }} Test Repository
baseurl  = https://{{ user }}:{{ password }}@pkg.opencast.org/rpms/testing/{{ os }}/{{ version }}/x86_64
enabled  = 1
gpgcheck = 1

[opencast-testing-noarch]
name = Opencast {{ os }} {{ version }} Test Repository - noarch
baseurl  = https://{{ user }}:{{ password }}@pkg.opencast.org/rpms/testing/{{ os }}/{{ version }}/noarch
enabled  = 1
gpgcheck = 1

[opencast-testing-debuginfo]
name = Opencast {{ os }} {{ version }} Test Repository - debuginfo
baseurl  = https://{{ user }}:{{ password }}@pkg.opencast.org/rpms/testing/{{ os }}/{{ version }}/debuginfo
enabled  = 0
gpgcheck = 1
