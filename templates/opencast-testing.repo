[opencast-testing]
name = Opencast {{ os }} {{ version }} Test Repository
baseurl  = https://pkg.opencast.org/rpms/testing/{{ os }}/{{ version }}/x86_64
username = {{ user }}
password = {{ password }}
enabled  = 1
gpgcheck = 1
gpgkey = https://pkg.opencast.org/gpgkeys/opencast-el7-rpm.opencast.org.key

[opencast-testing-noarch]
name = Opencast {{ os }} {{ version }} Test Repository - noarch
baseurl  = https://pkg.opencast.org/rpms/testing/{{ os }}/{{ version }}/noarch
username = {{ user }}
password = {{ password }}
enabled  = 1
gpgcheck = 1
gpgkey = https://pkg.opencast.org/gpgkeys/opencast-el7-rpm.opencast.org.key
